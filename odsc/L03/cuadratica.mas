	OUTPUT
	JUMP CONT
L0, LOAD ANS
	SKIPCOND 0C00
	JUMP L1
	LOAD z
	OUTPUT
	JUMP CONT
L1, LOAD O
	OUTPUT
CONT, LOAD N
	SUBT O
	STORE N
	JUMP LOOP

FINI, HALT

MUL, HEX 0
	CLEAR
	STORE Z
	LOAD Y
	STORE I
	LOAD O
	STORE DI
	LOAD Y
	SKIPCOND 000
	JUMP MUL_LOOP
	LOAD NO
